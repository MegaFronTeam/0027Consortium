mixin sPartners(data)
	// start sPartners
	+b.SECTION.sPartners.section#sPartners&attributes(attributes)
		.container
			+section-title(`Наши друзья`, `Партнеры`)
			+e.row.row
				mixin item(img)
					.col-6.col-md-4.col-lg-3.col-xxl-2
						+e.item
							.img-wrap-center
								+img(`img/sPartners-${img}.png`)
				+item(1)
				+item(2)
				+item(3)
				+item(4)
				+item(5)
				+item(6)
				+item(6)
				+item(5)
				+item(1)
				+item(3)
				+item(4)
				+item(2)
				
	// end sPartners